import java.util.ArrayList;
import java.util.List;

public class Box<T> {
	private T item;
	List<T> items = new ArrayList<>();
	public void add (T item) {;
		this.item = item;
		items.add(item);
	}
	
	public List<T> getItems(){
		System.out.println(items);
		/*for(T element : items) {
			System.out.println(element);
		}*/
		return items;
	}
	
}

