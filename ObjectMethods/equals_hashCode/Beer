package equals_hashCode;

public class Beer {
	private String name;
	private String origin;
	private double alcAmount;
	private double volume;

	public Beer(String name, String origin, double alcAmount, double volume) {
		super();
		this.name = name;
		this.origin = origin;
		this.alcAmount = alcAmount;
		this.volume = volume;
	}

	public String getName() {
		return name;
	}

	public String getOrigin() {
		return origin;
	}

	public double getAlcAmount() {
		return alcAmount;
	}

	public double getVolume() {
		return volume;
	}

	public boolean equals(Object obj) {
		if (obj == null) {
			return false;
		}
		if (obj == this) {
			return true;
		}
		if (!(obj instanceof Beer)) {
			return false;
		}
		Beer another = (Beer) obj;
		if (!name.equals(another.name)) {
			return false;
		}
		if (!origin.equals(another.origin)) {
			return false;
		}
		if (alcAmount != another.alcAmount) {
			return false;
		}
		if (volume != another.volume) {
			return false;
		}
		return true;
	}

	public boolean noNameEquals(Object obj) {
		if (obj == null) {
			return false;
		}

		if (obj == this) {
			return true;
		}

		if (!(obj instanceof Beer)) {
			return false;
		}
		Beer another = (Beer) obj;

		if (!origin.equalsIgnoreCase(another.origin)) {
			return false;
		}
		if (alcAmount != another.alcAmount) {
			return false;
		}
		if (volume != another.volume) {
			return false;
		}
		return true;
	}
	
	public int hashCode() {
		return 4*5;
	}

}
